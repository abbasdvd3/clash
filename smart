# ==================== 核心配置 ====================
mode: rule
mixed-port: 7890
ipv6: true
allow-lan: true
unified-delay: true
tcp-concurrent: true
log-level: error
bind-address: '*'
find-process-mode: strict
global-client-fingerprint: chrome
keep-alive-interval: 30
keep-alive-idle: 15
geodata-mode: true
geo-auto-update: true
geo-update-interval: 24
authentication:
skip-auth-prefixes:
  - 127.0.0.1/8
  - ::1/128
external-controller-cors:
  allow-private-network: true
  allow-origins:
  - http://tauri.localhost
  - https://yacd.metacubex.one
  - https://metacubex.github.io
  - https://board.zash.run.place
external-ui-url: https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip
external-ui-name: zashboard
external-ui: zashboard
external-controller: 127.0.0.1:9090
secret: 
geox-url:
  GeoIP: https://raw.githubusercontent.com/Chocolate4U/Iran-v2ray-rules/release/geoip.dat
  GeoSite: https://raw.githubusercontent.com/Chocolate4U/Iran-v2ray-rules/release/geosite.dat
  mmdb: https://raw.githubusercontent.com/Chocolate4U/Iran-clash-rules/release/geoip.metadb
  ANS: https://raw.githubusercontent.com/Chocolate4U/Iran-v2ray-rules/geolite2/GeoLite2-ASN.mmdb
profile:
  store-selected: true
  store-fake-ip: true

sniffer:
  enable: true
  sniff:
    HTTP:
      ports: [80, 8080-8880]
      override-destination: true
    TLS:
      ports: [443, 8443]
    QUIC:
      ports: [443, 8443]
  skip-domain:
    - "Mijia Cloud"
    - "+.push.apple.com"
    - "+.heytap.com"
    - "+.oppo.com"

hosts:
  doh.pub: [1.12.12.21, 120.53.53.53, 1.12.12.12]
  dot.pub: [1.12.12.21, 120.53.53.53, 1.12.12.12]
  "*.alidns.com": [223.5.5.5, 223.6.6.6, 2400:3200::1, 2400:3200:baba::1]
  dns.google: [8.8.8.8, 8.8.4.4, 2001:4860:4860::8888, 2001:4860:4860::8844]
  cloudflare-dns.com: [1.1.1.1, 1.0.0.1, 2606:4700:4700::1111, 2606:4700:4700::1001]

tun:
  enable: true
  stack: gvisor  
  device: Meta
  mtu: 9000
  dns-hijack:
    - "any:53"
    - "tcp://any:53"
  auto-route: true
  auto-redirect: true
  auto-detect-interface: true
  strict-route: true        
  endpoint-independent-nat: true
  exclude-uid: []
  exclude-package: []

# ==================== DNS 配置 ====================
dns:
  enable: true
  ipv6: true
  listen: 0.0.0.0:1053
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  cache-algorithm: arc
  proxy-server-nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  default-nameserver:
    - 223.5.5.5
    - 119.29.29.29

  nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  nameserver-policy:
    "geosite:ir,private,apple,": 
      - https://doh.pub/dns-query
      - https://dns.alidns.com/dns-query
    "geosite:geolocation-!cn,gfw,google,youtube,telegram,twitter,netflix": 
      - "https://1.1.1.1/dns-query#国外流量"
      - "https://8.8.8.8/dns-query#国外流量"
  fake-ip-filter:
    - rule-set:Direct
    - rule-set:Private
    - rule-set:China
    - "+.lan"
    - "+.local"
    - "+.market.xiaomi.com"
    - "+.heytap.com"
    - "+.oppo.com"
    - "xbox.*.microsoft.com"
    - "+.xboxlive.com"
    - "localhost.ptlogin2.qq.com"

